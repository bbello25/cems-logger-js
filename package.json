{
  "name": "@bbellovic/cems-logger-js",
  "version": "1.4.0",
  "description": "",
  "main": "dist/cjs/index.js",
  "typings": "dist/cjs/index.d.ts",
  "scripts": {
    "build": "npm run build:es2015 && npm run build:esm && npm run build:cjs && npm run build:umd && npm run build:umd:min",
    "build:es2015": "tsc --module es2015 --target es2015 --outDir dist/es2015",
    "build:esm": "tsc --module es2015 --target es5 --outDir dist/esm",
    "build:cjs": "tsc --module commonjs --target es5 --outDir dist/cjs",
    "build:umd": "rollup dist/esm/index.js --format umd --name CemsLogger --sourceMap --output dist/umd/CemsLogger.js",
    "build:umd:min": "cd dist/umd && uglifyjs --compress --mangle  --comments --o CemsLogger.min.js -- CemsLogger.js && gzip CemsLogger.min.js output=CemsLogger.min.js.gz"
  },
  "files": [
    "dist"
  ],
  "author": "Branislav Belloviƒç <b.bellovic@gmail.com> (https://github.com/bbello25)",
  "repository": {
    "url": "https://github.com/bbello25/cems-logger-js.git"
  },
  "keywords": [
    "javascript, server error logging, cems"
  ],
  "license": "ISC",
  "devDependencies": {
    "gzip-cli": "^0.1.3",
    "prettier": "1.15.3",
    "rollup": "^0.67.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typedoc": "^0.13.0",
    "typescript": "^3.2.1",
    "uglify-js": "^3.4.9"
  }
}
